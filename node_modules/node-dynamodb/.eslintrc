{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "plugins": [
    "filenames",
    "mocha"
  ],
  "rules": {
    "indent": [2, 2],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "keyword-spacing": 2,
    "no-trailing-spaces": 2,
    "semi": [2, "always"],
    "space-unary-ops": 2,
    "new-cap": 2,
    "array-bracket-spacing": [2, "never"],
    "block-scoped-var": 2,
    "computed-property-spacing": [2, "never"],
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "max-depth": [2, 4],
    "no-extend-native": 2,
    "no-use-before-define": [2, "nofunc"],
    "object-curly-spacing": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-before-blocks": [2, "always"],
    "space-infix-ops": 2,
    "no-unneeded-ternary": 2,
    "comma-style": [2, "last"],
    "comma-spacing": [2, { "before": false, "after": true }],
    "camelcase": 2,
    "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
    "no-multi-spaces": 2,
    "space-in-parens": [2, "never"],
    "no-eq-null": 2,
    "no-invalid-this": 2,
    "no-lone-blocks": 2,
    "no-new": 2,
    "no-octal": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": [2, "always"],
    "no-delete-var": 2,
    "no-shadow-restricted-names": 2,
    "no-extra-bind": 2,
    "no-throw-literal": 2,
    "quotes": [2, "single", "avoid-escape"],
    "quote-props": [2, "as-needed"],
    "no-new-wrappers": 2,
    "no-new-require": 2,
    "callback-return": 2,
    "mocha/no-exclusive-tests": 2,
    "require-yield": 0
  }
}
